<div id="<%= dom_id(post) %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= post.address %>
  </p>

  <p>
    <strong>Latitude:</strong>
    <%= post.latitude %>
  </p>

  <p>
    <strong>Longitude:</strong>
    <%= post.longitude %>
  </p>
</div>
